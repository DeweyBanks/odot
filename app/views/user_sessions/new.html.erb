<h1>Log In</h1>
<%= form_for :user_sessions_path do |f| %>
<%= f.label :email, "Email Address" %>
<%= f.text_field :email %>
<br />

<%= f.label :password, "password" %>
<%= f.password_field :password %>
<br />

<%= f.submit "Log In" %>
<%= link_to "Forgot Password?", new_password_reset_path %>
<% end %>
